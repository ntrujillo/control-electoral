<!DOCTYPE html>
<html class="no-js" data-ng-app="ControlElectoralApp">

<head>
    <title>
        Sistema de Control Electoral
    </title>
    <meta charset="utf-8">
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1">
    <meta id="load_styles_before">

    <!--Styles-->
    <!--<link rel="stylesheet" href="/styles/bootstrap/bootstrap.min.css">-->
    <link rel="stylesheet" href="/styles/bootstrap/dist/css/bootstrap.css">
    <link rel="stylesheet" href="/styles/perfect-scrollbar/css/perfect-scrollbar.css">
    <link rel="stylesheet" href="/styles/themes/default/font-awesome.css">
    <link rel="stylesheet" href="/styles/themes/default/urban.css">
    <link rel="stylesheet" href="/styles/themes/default/urban.skins.css">
    <!--end Styles-->


</head>
<body data-ng-controller="AppCtrl" class="{{ app.layout.sidebarTheme }} {{ app.layout.headerTheme }}">
<div class="app {{$state.current.data.appClasses}}"
     data-ng-class="{'layout-small-menu': app.layout.isSmallSidebar, 'layout-chat-open': app.layout.isChatOpen, 'layout-fixed-header': app.layout.isFixedHeader, 'layout-boxed': app.layout.isBoxed, 'layout-static-sidebar': app.layout.isStaticSidebar, 'layout-right-sidebar': app.layout.isRightSidebar, 'layout-fixed-footer': app.layout.isFixedFooter, 'message-open': app.isMessageOpen}"
     data-ui-view>
</div>

<script type="text/javascript">window.user = <%- user || 'null' %>;</script>

<!-- libs -->
<script type="text/javascript" src="/scripts/lib/jquery/dist/jquery.min.js"></script>
<script type="text/javascript" src="/scripts/lib/angular/angular.min.js"></script>
<script type="text/javascript" src="/scripts/lib/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
<script type="text/javascript" src="/scripts/lib/angular-ui-router/release/angular-ui-router.min.js"></script>
<script type="text/javascript" src="/scripts/lib/angular-resource/angular-resource.min.js"></script>
<script type="text/javascript" src="/scripts/lib/zingchart/client/zingchart.min.js"></script>
<script type="text/javascript" src="/scripts/lib/zingchart-angularjs/src/zingchart-angularjs.js"></script>
<script type="text/javascript" src="/scripts/lib/ngstorage/ngStorage.min.js"></script>
<script type="text/javascript" src="/scripts/lib/ocLazyLoad/dist/ocLazyLoad.min.js"></script>
<script type="text/javascript" src="/scripts/lib/perfect-scrollbar/js/perfect-scrollbar.jquery.js"></script>
<script type="text/javascript" src="/scripts/lib/fastclick/lib/fastclick.js"></script>
<script type="text/javascript" src="/scripts/app.js"></script>
<script type="text/javascript" src="/scripts/app.main.js"></script>
<script type="text/javascript" src="/scripts/config.router.js"></script>
<script type="text/javascript" src="/scripts/directives/navigation.js"></script>
<script type="text/javascript" src="/scripts/directives/offscreen.js"></script>
<script type="text/javascript" src="/scripts/directives/preloader.js"></script>


<!-- end libs -->

</body>

</html>